{
  "extends": [
    "airbnb",
    "eslint:recommended",
    "prettier",
    "next",
    "next/core-web-vitals"
  ],
  "plugins": ["prettier"],
  "rules": {
    "eqeqeq": "error",
    "no-console": "warn",
    "no-unused-vars": "error",
    "import/extensions": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".jsx", ".tsx"] }]
  }
}
